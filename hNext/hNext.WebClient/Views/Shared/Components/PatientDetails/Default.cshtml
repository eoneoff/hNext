@inject Microsoft.Extensions.Localization.IStringLocalizer<Resources> Localizer

<div class="main-panel ml-2">
    <table class="table table-borderless table-sm mt-1 mx-1">
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.FamilyName)]:</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.FirstName)]:</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.Patronimic)]:</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.Gender)]:</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.DateOfBirth)]:</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.CountryOfBirth)]:</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.PlaceOfBirth)]:</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap font-weight-bold">@Localizer[nameof(Resources.Address)]</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.Country)]:</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.Region)]:</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.District)]:</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.City)]:</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.Street)]:</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.Building)]:</td>
            <td class="side-tab-field"></td>
        </tr>
        <tr>
            <td class="text-right text-nowrap">@Localizer[nameof(Resources.Apartment)]:</td>
            <td class="side-tab-field"></td>
        </tr>
    </table>
</div>
