<template id = "case-history-prescriptions-template">
    <div class="m-3 d-flex flex-column">
        <prescription v-if="showAddPrescription" level="1" @@cancel="showAddPrescription=false"></prescription>
        <div>
        </div>
        <div class="mt-2">
            <button class="btn btn-primary w-100 mt-2" :disabled="!enabled"
                    @@click.stop.prevent="addPrescriptionClicked">
                <i class="fa fa-plus"></i>
            </button>
        </div>
    </div>
</template>